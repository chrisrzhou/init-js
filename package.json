{
  "name": "init-js",
  "version": "0.1.0",
  "description": "A simple config-driven CLI to initialize JS projects",
  "license": "MIT",
  "keywords": [
    "init",
    "js",
    "esm",
    "node"
  ],
  "repository": "chrisrzhou/init-js",
  "bugs": "https://github.com/chrisrzhou/init-js/issues",
  "author": "Chris Zhou <chrisrzhou@pm.me> (https://chrisrzhou.io)",
  "type": "module",
  "files": [
    ".github/",
    "lib/",
    ".editorconfig",
    ".gitignore",
    "cli.js",
    "config.json",
    "tsconfig.json"
  ],
  "bin": "./cli.js",
  "dependencies": {
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "xo": "^0.39.1"
  },
  "scripts": {
    "lint": "xo --fix",
    "test": "echo todo"
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": true,
    "trailingComma": "all"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-array-for-each": "off",
      "unicorn/no-array-reduce": "off"
    }
  }
}
