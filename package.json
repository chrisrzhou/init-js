{
  "name": "init-js",
  "version": "0.1.0",
  "description": "Initialize js projects",
  "license": "MIT",
  "keywords": [
    "initialize",
    "bootstrap",
    "js"
  ],
  "repository": "chrisrzhou/init-js",
  "bugs": "https://github.com/chrisrzhou/init-js/issues",
  "author": "Chris Zhou <chrisrzhou@pm.me> (https://chrisrzhou.io)",
  "type": "module",
  "files": [
    "cli.js",
    ".editorconfig",
    ".gitignore"
  ],
  "bin": "./cli.js",
  "dependencies": {},
  "devDependencies": {
    "xo": "^0.38.2"
  },
  "scripts": {
    "lint": "xo --fix"
  },
  "engines": {
    "node": ">=12.17.0"
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "trailingComma": "all",
    "useTabs": false
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "capitalized-comments": "off",
      "padding-line-between-statements": "off",
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-array-for-each": "off",
      "unicorn/no-array-reduce": "off",
      "unicorn/prefer-spread": "off"
    }
  }
}
